{
  "name": "@midstream/agentdb-integration-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for AgentDB + Midstreamer integration",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest unit/",
    "test:integration": "jest integration/",
    "test:bench": "jest benchmarks/ --testTimeout=60000",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "clean": "rm -rf dist coverage .jest-cache"
  },
  "keywords": [
    "agentdb",
    "midstreamer",
    "integration-tests",
    "temporal-ai"
  ],
  "author": "Midstream Team",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
